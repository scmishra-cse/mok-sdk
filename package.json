{
  "name": "@mok.one/sdk",
  "version": "0.7.0",
  "description": "",
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "module": "dist/app.mjs",
  "exports": {
    ".": "./dist/app.js",
    "./react": "./dist/react/react.es.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/app.d.ts"
      ]
    }
  },
  "scripts": {
    "lint": "tsc",
    "addFolder": "cd ./dist && mkdir react",
    "copyFiles": "copyfiles -f src/react/dist/*.js dist/react",
    "copyReactBuild": "(npm run addFolder && npm run copyFiles) || npm run copyFiles",
    "reactBuild": "cd src/react && npm i && npm run build",
    "build": "tsup src/app.ts --format cjs,esm --dts && npm run reactBuild  && npm run copyReactBuild",
    "release": "changeset publish"
  },
  "keywords": [],
  "author": "Unotag Gamification Platform",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.23.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "javascript-obfuscator": "^4.0.2",
    "tsup": "^6.5.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
